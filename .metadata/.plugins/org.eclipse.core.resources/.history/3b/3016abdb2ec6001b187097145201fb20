package com.sngular.devTraining2021.exercises;

public class AlgorithmName {
	public static String accum(String str) {
		// your code
		char[] c = str.toCharArray();
		for (int i = 0; i < c.length; i++) {
			acumulador(i, c[i]);
		}

		return null;
	}

	public static void acumulador(int pos, char l) {
	char capita=	 Character.toUpperCase(l);
	
	StringBuilder sb = new StringBuilder();
	sb.append(capita).append("-").append(l)
		.toString();
	
	System.out.println(sb);
		
	}
}